              <div class="earnings-features__items-item__title">{{ item.title }}</div>
              <div class="earnings-features__items-item__subtitle">{{ item.subtitle }}</div>
            </div>
          {% endfor %}
        </div>
        <div class="slider__header__arrows earnings-features__arrows">
